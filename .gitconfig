[core]
	commentchar = %
	
[color]
	ui = true	
	
[log]
	date = format:%d.%m.%Y %H:%M:%S
	
[alias]
	nah = "!git reset --hard ; git clean -df"
	aa = add .
	c = commit
	co = checkout
	s = status -sb
	b = branch
	l = "log --abbrev-commit --decorate --format=format:'%C(cyan)%h%C(reset) %C(green)%ad%C(reset) %C(white)%<|(120,trunc)%s%C(reset)  %aN%C(reset)'"
	rank = "shortlog -sn --no-merges"
	wip = "!git add --all; git checkout -b wip; git commit -m \"wip\"; git checkout develop;"
	pwip = "!git checkout wip; git reset --soft HEAD~1; git checkout develop; git branch -d wip; git reset"
	
[help]
	autocorrect = 1
